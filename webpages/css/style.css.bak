body {
	background: #1b1b1b;
	color: #ccc;
}

.ui.vertical.tabular.menu {
	border-right: 1px solid #585858;
}

.ui.vertical.tabular.menu .active.item{
	background: #2e2e2e;
	color: orangered;
	border-color: gray;
	font-family: 'Poppins', sans-serif;
}
.ui.form .field>label {
	color: gray;
}
.ui.grid>.column:not(.row) {
	padding-bottom: 0px;
}
.head-logo {
	border-bottom: 1px solid #585858;
	padding-bottom: .21428571rem;
	margin-top: -.14285714em;
	font-size: 32px;
	font-weight: 700;
	line-height: 1.28571429em;
	padding: 5px 0px 5px 0px;
	letter-spacing: .5px;
}
.ui.input {
	background: transparent;
}
h3 {
	color: firebrick;
}
.ui.input>input {
  width: 100%;
  padding: 10px 5px !important;
  margin: 8px 0;
  box-sizing: border-box;
  border: none !important;
  border-radius: 0px !important;
  border-bottom: 1px solid gray !important;
  background: transparent !important;
  color: white !important;
}

select {
  width: 100%;
  padding: 10px 5px !important;
  box-sizing: border-box;
  border: none !important;
  border-radius: 0px !important;
  border-bottom: 1px solid gray !important;
  background: transparent !important;
  color: white !important;
}

option {
	color: gray !important;
	padding: 10px 0px 10px 0px;
	height: 20px;
}

.ui.label {
	width: 35%;
  background-color: dimgray;
  padding: .5833em .833em;
  color: lightgray;
  font-weight: 700;
  text-align: right;
  -webkit-transition: background .1s ease;
}
.ui.checkbox label {
	color: white !important;
}

.ui.dividing.header {
	border-bottom: 1px solid #424949;
	color: darkcyan;
}

.ui.vertical.tabular.menu .item {
	color: #ccc;
	font-family: 'Poppins', sans-serif;
	font-size: small;
}

.right-pad {
	margin-right: 15px;
}

.left-pad {
  margin-left: 15px;
}

.ui.buttons .button:first-child {
  border-left: none;
  margin-left: 0;
  border-top-left-radius: 1.285714rem;
  border-bottom-left-radius: 1.285714rem;
  font-family: 'Poppins', sans-serif;
  font-weight: normal;
  color: white;
  background: dimgrey;
}

.ui.buttons .button:last-child {
  border-top-right-radius: 1.285714rem;
  border-bottom-right-radius: 1.285714rem;
  font-family: 'Poppins', sans-serif;
  font-weight: normal;
  color: white;
  background: dimgrey;
}

.ui.inverted.dimmer .ui.loader, .ui.loader {
	font-size: 13px !important;
}

.ui.segment {
	border: 2px solid dimgrey;
}

.log-head {
	font-size: large;
  font-weight: bold;
  color: darkcyan;
  letter-spacing: .5px;
  font-family: 'Poppins', sans-serif;
}

.net-text {
	font-size: 14px;
	font-weight: bold;
	line-height: 36px;
	font-family: 'Poppins', sans-serif;
}

.mandatory {
	color: red;
}

.pro-detail {
	float: right;
	font-size: 14px;
	font-weight: normal;
	margin-top: -10px;
	margin-right: 5px;
	text-transform: capitalize;
}

.pro-detail div {
	line-height: 30px;
	color: white;
}

#menus .disabled {
	color: #2e2e2e !important;
}

.inp-ltr {
	text-transform: capitalize;
}

.ui.tabular.menu .item {
	padding: 13px 5px;
}

.info-icon {
	cursor: pointer;
}

.net-note {
	font-weight: normal;
	font-size: small;
	color: red;
	margin-top: 5px;
}

.btnactive{
	background: orangered !important;
}

.switch {
  position: relative;
  display: inline-block;
  width: 75px;
  height: 25px;
}

.switch input {display:none;}

.slider {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #ca2222;
  -webkit-transition: .4s !important;
  transition: .4s;
  border-radius: 34px;
  padding-right: 80px;
}

.slider:before {
  position: absolute;
  content: "";
  height: 16px;
  width: 16px;
  left: 4px;
  bottom: 4px;
  background-color: white;
  -webkit-transition: .4s;
  transition: .4s;
  border-radius: 50%;
}

input:checked + .slider {
  background-color: #2ab934;
}

input:focus + .slider {
  box-shadow: 0 0 1px #2196F3;
}

input:checked + .slider:before {
  -webkit-transform: translateX(26px);
  -ms-transform: translateX(26px);
  transform: translateX(55px);
}

/*------ ADDED CSS ---------*/
.slider:after
{
 content:'OFF';
 color: white;
 display: block;
 position: absolute;
 transform: translate(-50%,-50%);
 top: 50%;
 left: 50%;
 font-size: 10px;
 font-family: Verdana, sans-serif;
}

input:checked + .slider:after
{  
  content:'ON';
}

/*--------- END --------*/